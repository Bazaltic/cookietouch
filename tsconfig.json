{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "removeComments": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "sourceRoot": "/",
    "strict": false,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["cookie/*"],
      "@logger": ["cookie/core/logger"],
      "@renderer/*": ["renderer/*"]
    }
  }
}
