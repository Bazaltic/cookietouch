{
  "name": "cookietouch",
  "version": "0.1.0-beta",
  "description": "DofusTouch Bot with Electron React/TypeScript",
  "main": "dist/main/main.js",
  "scripts": {
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir",
    "dev": "electron-webpack dev",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -t stylish",
    "lint:fix": "yarn lint --fix",
    "lint:check": "tslint-config-prettier-check ./tslint.json",
    "sentry": "node sentry-symbols.js",
    "postinstall": "electron-builder install-app-deps",
    "release": "yarn compile && build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cookie-project/cookietouch.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack",
    "cookie",
    "cookietouch"
  ],
  "author": "Christopher Yovanovitch <yovano_c@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cookie-project/cookietouch/issues"
  },
  "homepage": "https://github.com/cookie-project/cookietouch#readme",
  "devDependencies": {
    "@sentry/cli": "^1.36.4",
    "@sentry/webpack-plugin": "^1.6.1",
    "@types/node": "^10.12.9",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "awesome-typescript-loader": "^5.2.0",
    "electron": "^3.0.9",
    "electron-builder": "^20.36.2",
    "electron-download": "^4.1.1",
    "electron-webpack": "^2.6.0",
    "electron-webpack-ts": "^3.0.0",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "webpack": "^4.24.0",
    "webpack-build-notifier": "^0.1.30"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.1",
    "@sentry/electron": "^0.13.0",
    "anticaptcha": "^1.0.4",
    "axios": "^0.18.0",
    "downshift": "^3.1.4",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.4",
    "firebase": "^5.5.7",
    "linqts": "^1.12.2",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "react": "^16.6.3",
    "react-color": "^2.14.1",
    "react-dom": "^16.6.3",
    "react-loader-spinner": "^2.1.0",
    "source-map-support": "^0.5.9",
    "typeface-roboto": "^0.0.54",
    "ws": "^6.1.2"
  }
}
