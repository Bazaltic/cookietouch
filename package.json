{
  "name": "cookietouch",
  "version": "0.0.1-beta.2",
  "description": "DofusTouch Bot with Electron React/TypeScript",
  "main": "dist/main/main.js",
  "scripts": {
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir",
    "dev": "electron-webpack dev",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -t stylish",
    "lint:fix": "yarn lint --fix",
    "lint:check": "tslint-config-prettier-check ./tslint.json",
    "postinstall": "electron-builder install-app-deps",
    "release": "yarn compile && build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cookie-project/cookietouch.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack",
    "cookie",
    "cookietouch"
  ],
  "author": "Christopher Yovanovitch <yovano_c@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cookie-project/cookietouch/issues"
  },
  "homepage": "https://github.com/cookie-project/cookietouch#readme",
  "devDependencies": {
    "@types/node": "^10.7.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "awesome-typescript-loader": "^5.2.0",
    "electron": "^2.0.7",
    "electron-builder": "^20.28.1",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-build-notifier": "^0.1.27"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.2",
    "@material-ui/core": "^1.5.0",
    "@material-ui/icons": "^2.0.2",
    "anticaptcha": "^1.0.4",
    "axios": "^0.18.0",
    "downshift": "^2.1.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.1",
    "firebase": "^5.4.0",
    "linqts": "^1.12.0",
    "lodash": "^4.17.10",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "react": "^16.4.2",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.2",
    "react-loader-spinner": "^2.1.0",
    "source-map-support": "^0.5.8",
    "typeface-roboto": "^0.0.54",
    "ws": "^6.0.0"
  }
}
